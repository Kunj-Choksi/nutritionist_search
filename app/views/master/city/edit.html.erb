<h2>Edit City</h2>
    <div class="new-city-container">
    

<%= form_for(@city, :url => {:action => 'update', :id => @city.id }) do |f|%>
        <%= render(:partial => "form", :locals => {:f => f})%>

        <div class="form-buttons">
            <%= submit_tag("Done", :class => 'btn btn-primary') %>
        </div>
    <% end %>
</div>
